{
   size=800M
   rotate 7
   nomissingok
   notifempty
   sharedscripts
   postrotate
       /bin/kill -HUP `cat /usr/local/apache/logs/httpd.pid 2>/dev/null` 2> /dev/null || true
   endscript
}
